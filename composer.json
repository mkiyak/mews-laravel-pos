{
	"name": "mkiyak/mews-laravel-pos",
	"type": "package",
	"description": "Laravel 10 Pos Package",
	"keywords": ["laravel10 pos", "laravel10 sanal pos"],
	"homepage": "https://github.com/mkiyak/mews-laravel-pos",
	"license": "MIT",
	"authors": [
		{
			"name": "Mehmet",
			"email": "mkiyak511@gmail.com",
			"homepage": "https://github.com/mkiyak",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4|^8.1",
		"illuminate/config": "*",
		"mews/pos": "^0.14.0"
	},
	"require-dev": {
		"orchestra/testbench": "^8.12.2",
		"phpunit/phpunit": "^10.3.5"
	},
	"autoload": {
		"psr-4": {
			"Mews\\LaravelPos\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"Mews\\Test\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}
